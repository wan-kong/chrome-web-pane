{
  "name": "net-wanter",
  "displayName": "NetWanter",
  "version": "0.0.1",
  "author": "wankong",
  "description": "A powerful Chrome DevTools extension for advanced network request analysis and monitoring",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "devtools",
    "network-analyzer",
    "vue3",
    "vite",
    "typescript"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src --ext .vue,.ts,.tsx --fix",
    "fmt": "prettier --write '**/*.{vue,ts,json,css,scss,md}'",
    "clean": "rimraf build dist",
    "zip": "npm run build && node src/zip.js",
    "zip:dev": "npm run build:dev && node src/zip.js",
    "test": "echo 'No tests specified' && exit 0"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.26",
    "@tailwindcss/vite": "^4.1.10",
    "@types/chrome": "^0.0.246",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.20.1",
    "gulp": "^5.0.0",
    "gulp-zip": "^6.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.0",
    "sass": "^1.89.2",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vue-tsc": "~2.0.29"
  }
}
